{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "AllowLambdaToProcessLocalAreaTimesQueue",
            "Effect": "Allow",
            "Action": [
                "sqs:ReceiveMessage",
                "sqs:DeleteMessage",
                "sqs:GetQueueAttributes"
            ],
            "Resource": "arn:aws:sqs:us-east-1:677521478605:LocalAreaTimesNewsQueue"
        },
        {
            "Sid": "AllowLambdaToGetNewsImages",
            "Effect": "Allow",
            "Action": [
                "s3:GetObject"
            ],
            "Resource": "arn:aws:s3:::local-area-times-images/*"
        }
    ]
}